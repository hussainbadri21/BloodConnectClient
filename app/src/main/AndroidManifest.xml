<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hussain.bloodconnectclient">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splashS">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="com.example.hussain.bloodconnectclient.LoginActivity" />

                <category android:name="android.intent.category.default"/>
            </intent-filter>
        </activity>
        <activity
        android:name=".camps">

        <intent-filter>
            <action android:name="com.example.hussain.bloodconnectclient.camps" />

            <category android:name="android.intent.category.default" />
        </intent-filter>
        </activity>
        <activity
            android:name=".donorForm">

            <intent-filter>
                <action android:name="com.example.hussain.bloodconnectclient.donorForm" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity
            android:name=".done">

            <intent-filter>
                <action android:name="com.example.hussain.bloodconnectclient.done" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>